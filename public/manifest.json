{
  "manifest_version": 2,
  "version": "0.1.0",
  "minimum_chrome_version": "52",
  "icons": {
    "16": "icons/idle/icon-16.png",
    "24": "icons/idle/icon-24.png",
    "32": "icons/idle/icon-32.png",
    "48": "icons/idle/icon-48.png",
    "64": "icons/idle/icon-64.png",
    "128": "icons/idle/icon-128.png"
  },
  "name": "sous",
  "description": "Skip to the good part.",
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "contextMenus",
    "http://*/",
    "https://*/"
  ],
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icons/idle/icon-16.png",
      "24": "icons/idle/icon-24.png",
      "32": "icons/idle/icon-32.png",
      "48": "icons/idle/icon-48.png",
      "64": "icons/idle/icon-64.png",
      "128": "icons/idle/icon-128.png"
    },
    "default_title": "sous"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": []
}
